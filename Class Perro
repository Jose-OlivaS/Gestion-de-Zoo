public  class Perro extends Animal implements Alimentable{
public Perro(String nombre, int edad){
    super(nombre, edad);
}

@Override
public void hacerSonido(){
    System.out.println("El perro ladra");
    }
    public void comiendo() {
        System.out.println("El animal esta comiendo");
    }
        public void bebiendo(){
            System.out.println("El animal esta bebiendo");
        }
        public void hacerTruco(){
    System.out.println("El perro est√° haciendo un trucoo");
        }
    public void hacerTruco(String NombreTruco){
        System.out.println("El animal esta haciendo el truco de " + NombreTruco);
    }

}
